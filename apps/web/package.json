{
    "name": "web",
    "private": true,
    "packageManager": "pnpm@10.0.0",
    "version": "1.0.0-beta.3",
    "license": "MIT",
    "scripts": {
        "build": "NODE_OPTIONS='--max-old-space-size=4096' NUXT_BUILD_SSR=true NUXT_BUILD_ENV=production nuxi build --dotenv ../../.env.production.local && NUXT_BUILD_SSR=true node libs/release.mjs",
        "dev:pwa": "NUXT_BUILD_ENV=production VITE_PLUGIN_PWA=true nuxi dev --dotenv ../../.env.development.local",
        "dev": "NUXT_BUILD_SSR=true NUXT_BUILD_ENV=development nuxi dev --no-clear --dotenv ../../.env.development.local",
        "generate": "NODE_OPTIONS='--max-old-space-size=4096' NUXT_BUILD_SSR=false NUXT_BUILD_ENV=production nuxi generate --dotenv ../../.env.production.local && NUXT_BUILD_SSR=false node libs/release.mjs",
        "prepare": "NUXT_BUILD_SSR=true NUXT_BUILD_ENV=development nuxi prepare --dotenv ../../.env.development.local",
        "start": "node .output/server/index.mjs",
        "start:generate": "npx serve .output/public",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "typecheck": "vue-tsc --noEmit",
        "i18n:translate": "node libs/translate-i18n.mjs",
        "format": "prettier --write \"**/*.{js,ts,tsx,vue,json,css,scss,md}\""
    },
    "dependencies": {
        "@fastbuildai/designer": "workspace:*",
        "@fastbuildai/ui": "workspace:*",
        "@iconify-json/lucide": "^1.2.38",
        "@iconify-json/tabler": "^1.2.17",
        "@logicflow/core": "^2.0.16",
        "@lottiefiles/dotlottie-vue": "^0.5.8",
        "@nuxt/ui": "^3.1.2",
        "@nuxtjs/i18n": "^9.5.4",
        "@tailwindcss/vite": "^4.0.13",
        "@vueuse/core": "^13.1.0",
        "@vueuse/motion": "^3.0.3",
        "chalk": "^5.4.1",
        "cli-table3": "^0.6.5",
        "echarts": "^5.6.0",
        "katex": "^0.16.22",
        "libphonenumber-js": "^1.12.8",
        "markdown-it-github-alerts": "^1.0.0",
        "motion-v": "^1.0.0-beta.2",
        "nuxt": "^3.17.3",
        "nuxt-svgo": "^4.2.6",
        "reka-ui": "^2.2.0",
        "uuid": "^11.1.0",
        "vue-dompurify-html": "^5.3.0",
        "yup": "^1.6.1"
    },
    "devDependencies": {
        "@fastbuildai/config": "workspace:*",
        "@fastbuildai/http": "workspace:*",
        "@fastbuildai/utils": "workspace:*",
        "@nuxt/devtools": "^2.4.0",
        "@nuxt/eslint": "^1.3.0",
        "@nuxtjs/color-mode": "^3.5.2",
        "@pinia/nuxt": "^0.10.1",
        "@shikijs/markdown-it": "^3.7.0",
        "@tailwindcss/typography": "^0.5.16",
        "@types/markdown-it": "^14.1.2",
        "@typescript-eslint/eslint-plugin": "^8.30.1",
        "@typescript-eslint/parser": "^8.30.1",
        "@vite-pwa/nuxt": "^0.10.8",
        "@vueuse/nuxt": "^12.8.2",
        "clsx": "^2.1.1",
        "consola": "^3.4.0",
        "eslint": "^9.25.1",
        "eslint-plugin-format": "^1.0.1",
        "eslint-plugin-vue": "^10.0.0",
        "markdown-it-async": "^2.2.0",
        "pinia": "^3.0.1",
        "sass": "1.78.0",
        "tailwind-merge": "^3.0.2",
        "tailwindcss": "^4.0.13",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.8.3",
        "vue-eslint-parser": "^10.1.3",
        "vue-tsc": "^2.2.8"
    }
}
